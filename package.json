{
  "name": "vscode-yangcraft",
  "displayName": "YANGCraft - syntax highlighter and snippets",
  "description": "Yet Another Extension to work with YANG",
  "version": "0.0.1",
  "author": {
    "name": "Jairo R. Mejia Aponte",
    "email": "jairo.mejia.2708@gmail.com"
  },
  "contributors": [
    "Jairo R. Mejia Aponte"
  ],
  "license": "MIT",
  "publisher": "jrmejiaa",
  "icon": "",
  "bugs": {
    "url": "https://github.com/jrmejiaa/vscode-yangcraft/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jrmejiaa/vscode-yangcraft.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [{
      "id": "yang",
      "aliases": ["yang", "yang"],
      "extensions": ["yang"],
      "configuration": "./yang-configuration.jsonc"
    }],
    "grammars": [{
      "language": "yang",
      "scopeName": "source.yang",
      "path": "./syntaxes/yang.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "yang",
        "path": "./snippets/yang.code-snippets"
      }
    ]
  }
}
