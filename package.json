{
  "name": "vscode-yangcraft",
  "displayName": "vscode-yangcraft",
  "description": "Yet Another Extension to work with YANG",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [{
      "id": "yang",
      "aliases": ["yang", "yang"],
      "extensions": ["yang"],
      "configuration": "./yang-configuration.jsonc"
    }],
    "grammars": [{
      "language": "yang",
      "scopeName": "source.yang",
      "path": "./syntaxes/yang.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "yang",
        "path": "./snippets/yang.code-snippets"
      }
    ]
  }
}
